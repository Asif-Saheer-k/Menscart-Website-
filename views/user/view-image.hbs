<script>
function addToCart(proId){
      $.ajax({
            url:'/add-to-cart/'+proId,
                method:'get',
            success:(response)=>{
                if(response.status){
                        let count=$('#cartCount').html()
                        count=parseInt(count)+1
                        $("#cartCount").html(count)
                        swal("Status Updated",{icon:"success"})
                    }  else{
                      location.href='/login'
                    }
              

               
               
         
        }
  
})
}

function addWishlist(proId,userId){
    $.ajax({
        url:'/add-wishilist',
        data:{
            proId:proId,
            userId:userId,
        },
        method:'post',
        success:(response)=>{
            if(response.status){
                let Wcount=$('#wishilistCount').html()
                Wcount=parseInt(Wcount)+1
                $("#wishilistCount").html(Wcount)
            }
        }
})
}
</script>
