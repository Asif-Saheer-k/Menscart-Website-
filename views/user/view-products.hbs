            success:(response)=>{
                if(response.status){
                        let count=$('#cartCount').html()
                        count=parseInt(count)+1
                        $("#cartCount").html(count)
                        swal("Product Added To Cart",{icon:"success"})
                    }  else{
                      location.href='/login'
                    }
        }
  
})
}

function addWishlist(proId,userId){
    
    $.ajax({
        url:'/add-wishilist',
        data:{
            proId:proId,
            userId:userId,
        },
        method:'post',
        success:(response)=>{
            if(response.status){
